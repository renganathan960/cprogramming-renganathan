#include <stdio.h>

int main() {
   
    int arr[3][4] = {
        {1, 5, 2, 8},
        {3, 7, 6, 4},
        {7, 1, 9, 5}
    };
    int rows = 3;
    int cols = 4;

    for (int i = 0; i < rows; i++) {
   
        int max_val = arr[i][0];

        for (int j = 1; j < cols; j++) {
            
            if (arr[i][j] > max_val) {
                
                max_val = arr[i][j];
            }
        }


        printf("Maximum element of column %d is: %d\n", i, max_val);
    }

    return 0;
}
