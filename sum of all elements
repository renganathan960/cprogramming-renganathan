#include <stdio.h>

int main() {
    int a[3][3], sum = 0;

    // Read matrix
    for(int i = 0; i < 3; i++)
        for(int j = 0; j < 3; j++)
            scanf("%d", &a[i][j]);

    // Calculate sum
    for(int i = 0; i < 3; i++)
        for(int j = 0; j < 3; j++)
            sum += a[i][j];

    printf("%d", sum);

    return 0;
}
