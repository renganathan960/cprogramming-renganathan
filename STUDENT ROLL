#include <stdio.h>

int main() {
    int n;
    scanf("%d", &n);
    
    int sum_total = n * (n + 1) / 2; // sum of 1 to N
    int sum_arr = 0;
    
    for(int i = 0; i < n - 1; i++) {
        int x;
        scanf("%d", &x);
        sum_arr += x;
    }
    
    int missing = sum_total - sum_arr;
    printf("%d", missing);
    
    return 0;
}
